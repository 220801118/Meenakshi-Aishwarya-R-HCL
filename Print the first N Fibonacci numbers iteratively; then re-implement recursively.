ITERATIVE:
#include <stdio.h>

int main()
{
    int N, i;
    int a = 0, b = 1, c;

    scanf("%d", &N);

    if (N <= 0)
    {
        printf("Please enter a positive number\n");
    }
    else if (N == 1)
    {
        printf("0\n");
    }
    else
    {

        printf("0 1 ");

        for (i = 3; i <= N; i++)
        {
            c = a + b;
            printf("%d ", c);
            a = b;
            b = c;
        }
        printf("\n");
    }

    return 0;
}

RECURSIVE:
#include <stdio.h>

int fib(int n)
{
    if (n == 0)
        return 0;
    else if (n == 1)
        return 1;
    else
        return fib(n - 1) + fib(n - 2);
}

int main()
{
    int N, i;

    scanf("%d", &N);

    if (N <= 0)
    {
        printf("Please enter a positive number\n");
    }
    else
    {
  
        for (i = 0; i < N; i++)
        {
            printf("%d ", fib(i));
        }
        printf("\n");
    }

    return 0;
}


OUTPUT:
7

0 1 1 2 3 5 8

