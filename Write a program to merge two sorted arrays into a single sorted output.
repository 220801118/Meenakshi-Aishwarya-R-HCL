#include <stdio.h>

int main()
{
    int n, m;
    int a[100], b[100], c[200];
    int i = 0, j = 0, k = 0;

    // Read sizes
    scanf("%d %d", &n, &m);

    // Read first sorted array
    for (i = 0; i < n; i++)
    {
        scanf("%d", &a[i]);
    }

    // Read second sorted array
    for (j = 0; j < m; j++)
    {
        scanf("%d", &b[j]);
    }

    i = 0; 
    j = 0;

    // Merge both arrays
    while (i < n && j < m)
    {
        if (a[i] <= b[j])
        {
            c[k] = a[i];
            i++;
        }
        else
        {
            c[k] = b[j];
            j++;
        }
        k++;
    }

    // Copy remaining elements of a[]
    while (i < n)
    {
        c[k] = a[i];
        i++;
        k++;
    }

    // Copy remaining elements of b[]
    while (j < m)
    {
        c[k] = b[j];
        j++;
        k++;
    }

    // Print merged array
    printf("Merged array: ");
    for (i = 0; i < n + m; i++)
    {
        printf("%d ", c[i]);
    }
    printf("\n");

    return 0;
}
